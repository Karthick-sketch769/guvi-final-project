pipeline {
agent any 
    stages {
     stage ('git clone'){
       steps  {
          sh 'https://github.com/Karthick-sketch769/guvi-final-project.git'
           }
           }
    stage ('build') {
      steps {
        sh './build.sh'
           }
           }
     stage ('deploy') {
     steps {
        sh './deploy.sh'
        }
        }
        }
        }
